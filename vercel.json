{
  "headers": [
    {
      "source": "/(.*\\.xml)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.xml$).*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "https://www.lucaslucas.work/:path*",
      "destination": "https://lucaslucas.work/:path*",
      "permanent": true
    }
  ]
}
