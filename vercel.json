{
  "headers": [
    {
      "source": "/(.*\\.xml)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.xml$).*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.lucaslucas.work"
        }
      ],
      "destination": "https://lucaslucas.work/:path*",
      "permanent": true
    }
  ]
}
